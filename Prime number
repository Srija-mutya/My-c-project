#include <stdio.h>
void main()
{
    int n,i,f;
    printf("enter any number:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
          f++;
    }
    if(f==2)
      printf("The given number is a prime number");
    else
      printf("The given number is not a prime number");
     
}
