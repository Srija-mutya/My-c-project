#include<stdio.h>
#include<math.h>
void main()
{
int a,b,c,d,x1,x2;
printf("enter the values");
scanf("%d%d%d",&a,&b,&c);
printf("\n%d*x*x+%d*x+%d",a,b,c);
d=b*b-4*a*c;
x1=(-b+sqrt(d))/2*a;
x2=(-b-sqrt(d))/2*a;
if(d>=0)
{
printf("\nthe roots are real");
printf("\n%d\n%d",x1,x2);
}
else
{ 
printf("\nthe roots are imaginary");
}
}
