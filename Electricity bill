#include<stdio.h>
void main()
{
int U;
printf("Enter the units:");
scanf("%d",&U);
if(U<=50)
 {
  printf("The electricity bill is 0.50 RS");
 }
else if(U>50,U<=150)
 {
  printf("The electricity bill is 0.75 RS");
 }
 else if(U>150,U<=250)
 { 
  printf("The electricity bill is 1.20 RS");
 }
 else
 {
 printf("The electricity bill is 1.50 RS");
 } 
}
    
